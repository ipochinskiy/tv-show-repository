{
  "root": true,
  "extends": "airbnb",
  "env": {
    "es6": true,
  },
  "ecmaFeatures": {
    "modules": true,
  },
  "rules": {
  	"no-console": 0,
    "func-names": 0,
    "no-param-reassign": 0,
    "indent": [ "error", "tab" ],
  },
  "globals": {
    "angular": true,
    "moment": true,
    "NProgress": true,
    "FB": true,
    "document": true,
  },
}

